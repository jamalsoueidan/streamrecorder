{
  "kind": "collectionType",
  "collectionName": "followers",
  "info": {
    "singularName": "follower",
    "pluralName": "followers",
    "displayName": "Follower"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nickname": {
      "type": "string"
    },
    "username": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "default": "tiktok",
      "enum": [
        "tiktok",
        "twitch"
      ]
    },
    "protected": {
      "type": "boolean"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "unknown"
      ]
    },
    "country": {
      "type": "string"
    },
    "countryCode": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "languageCode": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "lastCheckedAt": {
      "type": "datetime"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "followers"
    },
    "recordings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recording.recording",
      "mappedBy": "follower"
    }
  }
}
