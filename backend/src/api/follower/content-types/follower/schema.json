{
  "kind": "collectionType",
  "collectionName": "followers",
  "info": {
    "singularName": "follower",
    "pluralName": "followers",
    "displayName": "Follower"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "default": "tiktok",
      "enum": [
        "tiktok",
        "twitch"
      ]
    },
    "protected": {
      "type": "boolean"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "followers"
    },
    "recordings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recording.recording",
      "mappedBy": "follower"
    }
  }
}
