{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn generateCmsTypes && next build",
    "start": "next start",
    "lint": "eslint",
    "generateCmsTypes": "swagger-typescript-api generate -p ../backend/src/extensions/documentation/documentation/1.0.0/full_documentation.json -o ./src/lib/types -n strapi-autogenerated.ts --no-client --route-types --union-enums --module-name-first-tag --extract-request-params --extract-request-body --extract-response-body --extract-enums"
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "swagger-typescript-api": "^13.2.16"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "typescript": "^5"
  }
}
